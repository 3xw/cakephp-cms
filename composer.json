{
  "name": "3xw/cakephp-cms",
  "description": "CakePHP pages plugins",
  "type": "cakephp-plugin",
  "require": {
    "php": ">=7.2",
    "cakephp/cakephp": "^4.0",
    "3xw/cakephp-tinymce": "^4.0",
    "3xw/cakephp-attachment": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Trois\\Cms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trois\\Cms\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  }
}
